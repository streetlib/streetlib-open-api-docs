        in: query
        required: false
        description: filter list by Payment Account
        schema:
          type: string
          format: uuid
      - name: page
        in: query
        required: false
        description: page to display
        schema:
          type: integer
          minimum: 1
          default: 1
      - name: per_page
        in: query
        required: false
        description: items per page
        schema:
          type: integer
          default: 10
          maximum: 100
      responses:
        '200':
